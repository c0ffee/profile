! tags as defined in X11/keysymdef.h
! syntax:
! keycode <int> = <no-modifier> [<Shift> [<Mode_switch> [<Shift+Mode_switch>]]]
! map Caps-Lock to Mode_switch
!remove Lock = Caps_Lock
keycode 66 = Mode_switch
keycode 135 = Super_L
keycode 10 = 1 exclam           onesuperior     onesubscript
keycode 11 = 2 at               twosuperior     twosubscript
keycode 12 = 3 numbersign       threesuperior   threesubscript
keycode 13 = 4 dollar           foursuperior    foursubscript
keycode 14 = 5 percent          fivesuperior    fivesubscript
keycode 15 = 6 asciicircum      sixsuperior     sixsubscript
keycode 16 = 7 ampersand        sevensuperior   sevensubscript
keycode 17 = 8 asterisk         eightsuperior   eightsubscript
keycode 18 = 9 parenleft        ninesuperior    ninesubscript
keycode 19 = 0 parenright       zerosuperior    zerosubscript
keycode 20 = minus underscore   emdash          endash
keycode 21 = equal plus         notsign
keycode 34 = bracketleft braceleft              dagger doubledagger
keycode 35 = bracketright braceright            enopensquarebullet enfilledsqbullet
keycode 36 = Return Return Multi_key Multi_key
keycode 24 = q Q        Greek_omega Greek_OMEGA
keycode 26 = e E        ediaeresis Ediaeresis
keycode 29 = y Y        ydiaeresis Ydiaeresis
keycode 30 = u U        udiaeresis Udiaeresis
keycode 31 = i I        idiaeresis Idiaeresis
keycode 32 = o O        odiaeresis Odiaeresis
keycode 33 = p P        Greek_pi paragraph
keycode 38 = a A        adiaeresis Adiaeresis
keycode 39 = s S        ssharp section
keycode 46 = l L        lstroke Lstroke
keycode 54 = c C        EuroSign cent
keycode 57 = n N        ntilde Ntilde
keycode 58 = m M        Greek_mu Greek_MU
keycode 59 = comma less                         leftsinglequotemark leftdoublequotemark
keycode 60 = period greater                     rightsinglequotemark rightdoublequotemark
keycode 61 = slash question                     exclamdown questiondown
keycode 49 = grave asciitilde                   degree division
keycode 94 = guillemotleft guillemotright       malesymbol femalesymbol
